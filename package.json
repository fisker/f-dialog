  "main": "lib/full-version/dialog.common.js",
  "module": "lib/full-version/dialog.esm.min.mjs",
  "umd:main": "lib/full-version/dialog.min.js",
  "jsdelivr": "lib/full-version/dialog.min.js",
  "description": "fisker's dialog",
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@commitlint/cli": "8.1.0",
    "@fisker/commitlint-config": "1.1.2",
    "@fisker/eslint-config": "1.2.14",
    "@fisker/husky-config": "1.1.1",
    "@fisker/lint-staged-config": "1.0.5",
    "@fisker/postcss-config": "1.1.2",
    "@fisker/prettier-config": "1.0.18",
    "@fisker/stylelint-config": "1.1.9",
    "babel-plugin-transform-es3-member-expression-literals": "6.22.0",
    "babel-plugin-transform-es3-property-literals": "6.22.0",
    "babel-plugin-transform-es5-property-mutators": "6.24.1",
    "caniuse-lite": "1.0.30000975",
    "cp-file": "7.0.0",
    "create-banner": "1.0.0",
    "cssnano-cli": "1.0.5",
    "cz-conventional-changelog-emoji": "0.1.0",
    "dialog-polyfill": "0.5.0",
    "es6-promise": "4.2.8",
    "eslint": "6.2.2",
    "gzip-size": "5.1.1",
    "husky": "3.0.4",
    "jest": "24.9.0",
    "jsdom": "15.1.1",
    "lint-staged": "9.2.4",
    "markdownlint-cli": "0.18.0",
    "mem": "5.1.1",
    "npm-run-all": "4.1.5",
    "postcss-cli": "6.1.3",
    "postcss-header": "1.0.0",
    "postcss-pseudo-element-colons": "1.0.2",
    "prettier": "1.18.2",
    "prettier-config-fisker": "1.0.3",
    "pretty-bytes": "5.3.0",
    "read-dir-deep": "7.0.1",
    "rimraf": "3.0.0",
    "rollup": "1.20.1",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.0.2",
    "@rollup/plugin-json": "4.0.0",
    "@rollup/plugin-node-resolve": "6.0.0",
    "rollup-plugin-prettier": "0.6.0",
    "rollup-plugin-terser": "5.1.1",
    "sass": "1.22.10",
    "stylelint": "10.1.0",
    "table": "5.4.6",
    "uglify-js": "3.6.0",
    "write-pkg": "4.0.0"
  },
  "files": [
    "lib",
    "src"
  ],
  "keywords": [
    "dialog",
    "alert",
    "confirm",
    "prompt",
    "promise"
  ],
  "license": "MIT",
  "name": "fu-dialog",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fisker/fu-dialog.git"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:css": "bash ./scripts/build-css.sh",
    "build:docs": "parcel build demo/index.html --out-dir docs",
    "build:js": "bash ./scripts/build-js.sh",
    "build:package": "bash ./scripts/build-package.sh",
    "build:styles": "bash ./scripts/build-styles-source.sh",
    "clean": "rimraf lib",
    "dev": "parcel demo/index.html --open",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --write",
    "format:stylelint": "yarn lint:stylelint --fix",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "lint:stylelint": "stylelint \"**/*.{css,scss,less}\"",
    "release": "run-s clean lint build test format dist",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "version": "yarn build && git add lib"
  },
  "version": "1.0.11",
  "bugs": {
    "url": "https://github.com/fisker/fu-dialog/issues"
  },
  "homepage": "https://github.com/fisker/fu-dialog#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-emoji"
    }
  },
  "sideEffects": false
}
