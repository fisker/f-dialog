{
  "name": "fu-dialog",
  "version": "1.1.0",
  "description": "fisker's dialog",
  "keywords": [
    "alert",
    "confirm",
    "dialog",
    "promise",
    "prompt"
  ],
  "homepage": "https://github.com/fisker/fu-dialog#readme",
  "bugs": {
    "url": "https://github.com/fisker/fu-dialog/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fisker/fu-dialog.git"
  },
  "license": "MIT",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "main": "dist/full-version/dialog.common.js",
  "umd:main": "dist/full-version/dialog.min.js",
  "jsdelivr": "dist/full-version/dialog.min.js",
  "unpkg": "dist/full-version/dialog.min.js",
  "module": "dist/full-version/dialog.esm.min.mjs",
  "browser": "dist/full-version/dialog.min.js",
  "style": "dist/full-version/dialog.min.css",
  "scripts": {
    "build": "run-p build:*",
    "build:css": "bash ./scripts/build-css.sh",
    "build:docs": "parcel build demo/index.html --out-dir docs",
    "build:js": "bash ./scripts/build-js.sh",
    "build:package": "bash ./scripts/build-package.sh",
    "build:styles": "bash ./scripts/build-styles-source.sh",
    "clean": "run-p clean:*",
    "clean:dist": "del-cli dist",
    "clean:docs": "del-cli docs",
    "dev": "parcel demo/index.html --open",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:markdown": "yarn lint:markdown --fix",
    "format:package-json": "sort-package-json \"package.json\" \"packages/*/package.json\"",
    "format:prettier": "yarn lint:prettier --write",
    "format:stylelint": "yarn lint:stylelint --fix",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:package-json": "yarn run format:package-json --check",
    "lint:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "lint:stylelint": "stylelint \"**/*.{css,scss,less}\"",
    "release": "run-s clean lint build test format dist",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "version": "yarn build && git add dist"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/core": "7.7.5",
    "@babel/preset-env": "7.7.7",
    "@commitlint/cli": "8.2.0",
    "@fisker/commitlint-config": "1.1.4",
    "@fisker/eslint-config": "1.5.4",
    "@fisker/husky-config": "1.1.5",
    "@fisker/lint-staged-config": "1.0.8",
    "@fisker/postcss-config": "1.1.4",
    "@fisker/prettier-config": "1.0.24",
    "@fisker/stylelint-config": "1.2.0",
    "@rollup/plugin-json": "4.0.0",
    "@rollup/plugin-node-resolve": "6.0.0",
    "babel-plugin-transform-es3-member-expression-literals": "6.22.0",
    "babel-plugin-transform-es3-property-literals": "6.22.0",
    "babel-plugin-transform-es5-property-mutators": "6.24.1",
    "caniuse-lite": "1.0.30001015",
    "cp-file": "7.0.0",
    "create-banner": "1.0.0",
    "cssnano-cli": "1.0.5",
    "cz-conventional-changelog": "3.0.2",
    "del-cli": "3.0.0",
    "dialog-polyfill": "0.5.0",
    "es6-promise": "4.2.8",
    "eslint": "6.7.2",
    "gzip-size": "5.1.1",
    "husky": "3.1.0",
    "jest": "24.9.0",
    "jsdom": "15.2.1",
    "lint-staged": "9.5.0",
    "markdownlint-cli": "0.20.0",
    "mem": "6.0.1",
    "npm-run-all": "4.1.5",
    "parcel-bundler": "1.12.4",
    "postcss-cli": "6.1.3",
    "postcss-header": "1.0.0",
    "postcss-pseudo-element-colons": "1.0.2",
    "prettier": "1.19.1",
    "prettier-config-fisker": "1.0.3",
    "pretty-bytes": "5.3.0",
    "read-dir-deep": "7.0.1",
    "rimraf": "3.0.0",
    "rollup": "1.27.13",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-prettier": "0.6.0",
    "rollup-plugin-terser": "5.1.3",
    "sass": "1.23.7",
    "sort-package-json": "1.31.0",
    "stylelint": "12.0.0",
    "table": "5.4.6",
    "uglify-js": "3.7.2",
    "write-pkg": "4.0.0"
  },
  "resolutions": {
    "browserslist": "4.8.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
